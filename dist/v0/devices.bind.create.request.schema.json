{
  "$id": "https://schemas.tillhub.com/v0/devices.bind.create.request.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "type": "object",
  "required": [
    "token",
    "client_account"
  ],
  "properties": {
    "token": {
      "type": "string"
    },
    "client_account": {
      "type": "string"
    },
    "register": {
      "type": "string",
      "format": "uuid"
    }
  }
}

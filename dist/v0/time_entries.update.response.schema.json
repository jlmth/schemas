{
  "$id": "https://schemas.tillhub.com/v0/time_entries.update.response.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": true,
  "type": "object",
  "required": [
    "id"
  ],
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "day",
        "break"
      ]
    },
    "description": {
      "anyOf": [
        {
          "type": "string",
          "maxLength": 32,
          "minLength": 1
        },
        {
          "type": "null"
        }
      ]
    },
    "start": {
      "anyOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "null"
        }
      ]
    },
    "end": {
      "anyOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "null"
        }
      ]
    },
    "staff_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "id": {
      "type": "string",
      "maxLength": 128
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}

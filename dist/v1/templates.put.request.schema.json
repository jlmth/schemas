{
  "$id": "https://schemas.tillhub.com/v1/templates.put.request.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "type": "object",
  "required": [],
  "properties": {
    "name": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 64
        },
        {
          "type": "null"
        }
      ]
    },
    "type": {
      "type": "string",
      "enum": [
        "invoice_v1",
        "delivery_notes_v1",
        "full_receipt_v1"
      ]
    },
    "active": {
      "type": "boolean"
    },
    "deleted": {
      "type": "boolean"
    }
  }
}
